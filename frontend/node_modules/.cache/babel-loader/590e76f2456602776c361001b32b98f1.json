alStorage.getItem('token')\n    config.headers['Authorization'] = `Bearer ${token}`\n    return config\n})\n\nclass LeaveAPI{\n\n    async get(leaveID){\n        if(leaveID){\n            const response = await request.get(`${url}/${leaveID}`)\n            return response.data\n        }else{\n            const response = await request.get(url)\n            return response.data\n        }\n    }\n\n    async create(data){\n        const response = await request.post(`${url}/create`, data)\n        return response.data\n    }\n\n    async update(leaveID, data){\n        const response = await request.put(`${url}/update/${leaveID}`, data)\n        return response.data\n    }\n\n    async delete(leaveID){\n        const response = await request.delete(`${url}/${leaveID}/delete`)\n        return response.data\n    }\n}\n\n\nexport default  new LeaveAPI();"]}]}