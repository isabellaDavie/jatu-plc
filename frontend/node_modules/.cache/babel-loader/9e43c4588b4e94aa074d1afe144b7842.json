()\n            }else{\n                this.loading = !this.loading\n                this.reset()\n                this.$emit('success')\n            }\n        },\n        \n        reset(){\n            this.comment.body= null\n        }\n    }\n}\n</script>\n\n\n<style lang=\"scss\" scoped>\n\n\n</style>"],"sourceRoot":"src/components/forms"}]}